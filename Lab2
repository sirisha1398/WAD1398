    <script>
        function applyStyle(styleId) {
            const styles = document.querySelectorAll('style');
            styles.forEach(style => { style.disabled = true; });
            const selectedStyle = document.getElementById(styleId);
            selectedStyle.disabled = false;
        }
    </script>
